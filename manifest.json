{
  "name": "Atcoder extension",
  "version": "1.0",
  "icon": {
    "128": "logo_128.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://atcoder.jp/contests/*/tasks"],
      "js": ["content.js","popup.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_title":"Atcoder extension"
  },
  "permissions": ["tabs"]
}